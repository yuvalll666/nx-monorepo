{
    "extends": "../tsconfig.base.json",
    "compilerOptions": {
        "target": "es2015",
        "module": "esnext",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strictPropertyInitialization": false
    },
    "include": ["**/*.ts"]
}
